@model IEnumerable<Ingogo.Model.Animal_Management.Model_View.FeedingPercentageView>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>GetAllFeedPercentage</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "AddFeedPercentage")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Percentage)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Percentage)
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
